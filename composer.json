{
    "name": "tobyz/json-api-server",
    "description": "A fully automated JSON:API server implementation in PHP.",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "asispts/http-accept": "^1.0",
        "doctrine/inflector": "^2.0",
        "nyholm/psr7": "^1.8",
        "psr/http-message": "^1.0|^2.0",
        "psr/http-server-handler": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Toby Zerner",
            "email": "toby.zerner@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tobyz\\JsonApiServer\\": "src/"
        },
        "files": [
            "src/functions.php",
            "src/Laravel/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tobyz\\Tests\\JsonApiServer\\": "tests/"
        }
    },
    "require-dev": {
        "dms/phpunit-arraysubset-asserts": "^0.5.0",
        "phpbench/phpbench": "^1.2",
        "phpunit/phpunit": "^10.2.2"
    },
    "scripts": {
        "test": "phpunit",
        "bench": "vendor/bin/phpbench run --report=default",
        "bench:baseline": "vendor/bin/phpbench run --tag=baseline --report=default",
        "bench:compare": "vendor/bin/phpbench run --ref=baseline --report=default"
    },
    "config": {
        "sort-packages": true
    }
}
